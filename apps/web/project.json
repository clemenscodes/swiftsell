{
    "name": "web",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/web",
    "projectType": "application",
    "targets": {
        "env": {
            "executor": "nx:run-commands",
            "defaultConfiguration": "development",
            "configurations": {
                "development": {
                    "command": "apps/web/scripts/env.sh development"
                },
                "production": {
                    "command": "apps/web/scripts/env.sh production"
                }
            }
        },
        "build": {
            "dependsOn": ["env", "prisma-generate"],
            "executor": "@nrwl/next:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "root": "apps/web",
                "outputPath": "dist/apps/web",
                "assets": [
                    {
                        "input": "libs/shared/assets",
                        "glob": "**/*",
                        "output": ""
                    }
                ]
            }
        },
        "start": {
            "dependsOn": ["build"],
            "executor": "@nrwl/next:server",
            "defaultConfiguration": "development",
            "configurations": {
                "development": {
                    "buildTarget": "web:build:development",
                    "dev": true
                },
                "production": {
                    "buildTarget": "web:build:production",
                    "dev": false
                }
            }
        },
        "seed": {
            "command": "nx prisma-seed web --skip-nx-cache"
        },
        "dev": {
            "dependsOn": ["seed"],
            "executor": "nx:run-commands",
            "defaultConfiguration": "development",
            "configurations": {
                "development": {
                    "command": "nx run-many --parallel=3 --target=start --output-style=stream --projects=web,firebase swiftsell-development"
                },
                "production": {
                    "command": "nx start web --configuration=production"
                }
            }
        },
        "export": {
            "executor": "@nrwl/next:export",
            "options": {
                "buildTarget": "web:build:production"
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "apps/web/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/web/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "analyze": {
            "command": "ANALYZE=true nx build web --skip-nx-cache"
        },
        "docker": {
            "executor": "@nx-tools/nx-container:build",
            "defaultConfiguration": "local",
            "configurations": {
                "local": {
                    "engine": "docker",
                    "load": true,
                    "metadata": {
                        "images": [],
                        "tags": ["type=schedule", "type=sha"]
                    }
                },
                "ci": {
                    "engine": "docker",
                    "load": true,
                    "cache-from": ["type=gha"],
                    "cache-to": ["type=gha,mode=max"],
                    "metadata": {
                        "images": [],
                        "tags": ["type=schedule", "type=sha"]
                    }
                }
            }
        },
        "deploy": {
            "executor": "nx:run-commands",
            "defaultConfiguration": "development",
            "configurations": {
                "development": {
                    "command": "apps/web/scripts/deploy.sh development"
                },
                "production": {
                    "command": "apps/web/scripts/deploy.sh production"
                }
            }
        },

        "prisma-generate": {
            "executor": "nx:run-commands",
            "options": {
                "command": "PRISMA_DISABLE_WARNINGS=1 yarn prisma generate --schema=prisma/schema.prisma",
                "cwd": "apps/web"
            }
        },
        "prisma-migrate": {
            "dependsOn": ["prisma-generate", "^start"],
            "executor": "nx:run-commands",
            "options": {
                "command": "PRISMA_DISABLE_WARNINGS=1 yarn prisma migrate dev --schema=prisma/schema.prisma --name $(git rev-parse --short HEAD)",
                "cwd": "apps/web"
            }
        },
        "prisma-seed": {
            "dependsOn": ["prisma-migrate"],
            "executor": "nx:run-commands",
            "options": {
                "command": "PRISMA_DISABLE_WARNINGS=1 yarn ts-node prisma/seed.ts",
                "cwd": "apps/web"
            }
        },
        "prisma-pull": {
            "executor": "nx:run-commands",
            "options": {
                "command": "PRISMA_DISABLE_WARNINGS=1 yarn prisma db pull --schema=prisma/schema.prisma",
                "cwd": "apps/web"
            }
        },
        "prisma-push": {
            "executor": "nx:run-commands",
            "options": {
                "command": "PRISMA_DISABLE_WARNINGS=1 yarn prisma db push --schema=prisma/schema.prisma",
                "cwd": "apps/web"
            }
        },
        "prisma-deploy": {
            "executor": "nx:run-commands",
            "options": {
                "command": "PRISMA_DISABLE_WARNINGS=1 yarn prisma migrate deploy --schema=prisma/schema.prisma",
                "cwd": "apps/web"
            }
        },
        "prisma-status": {
            "executor": "nx:run-commands",
            "options": {
                "command": "PRISMA_DISABLE_WARNINGS=1 yarn prisma migrate status --schema=prisma/schema.prisma",
                "cwd": "apps/web"
            }
        },
        "prisma-studio": {
            "executor": "nx:run-commands",
            "options": {
                "command": "PRISMA_DISABLE_WARNINGS=1 yarn prisma studio --schema=prisma/schema.prisma",
                "cwd": "apps/web"
            }
        },
        "build-custom-server": {
            "executor": "@nrwl/js:tsc",
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/apps/web/.next/standalone/apps/web/server",
                "main": "apps/web/server.ts",
                "tsConfig": "apps/web/tsconfig.server.json",
                "clean": false,
                "assets": []
            },
            "configurations": {
                "development": {},
                "production": {}
            }
        },
        "server": {
            "command": "apps/web/scripts/server.sh"
        },
        "serve": {
            "command": "apps/web/scripts/standalone.sh"
        }
    },
    "implicitDependencies": [
        "infra-run",
        "db",
        "infra-wif",
        "infra-firebase",
        "infra-artifact",
        "infra-bucket-cdn",
        "infra-bucket-state",
        "infra-bucket-isr"
    ],
    "tags": []
}
