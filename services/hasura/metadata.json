            "name": "cart",
            "schema": "public"
          }
        },
        {
          "table": {
            "name": "inventory",
            "schema": "public"
          }
        },
        {
          "table": {
            "name": "order",
            "schema": "public"
          }
        },
        {
          "table": {
            "name": "product",
            "schema": "public"
          }
        },
        {
          "table": {
            "name": "store",
            "schema": "public"
          }
        },
        {
          "table": {
            "name": "user",
            "schema": "public"
          }
        }
      ],
      "configuration": {
        "connection_info": {
          "database_url": {
            "from_env": "HASURA_GRAPHQL_DATABASE_URL"
