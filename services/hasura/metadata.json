{
  "version": 3,
  "sources": [
    {
      "name": "default",
      "kind": "postgres",
      "tables": [
        {
          "table": {
            "name": "Product",
            "schema": "public"
          },
          "object_relationships": [
            {
              "name": "Product",
              "using": {
                "foreign_key_constraint_on": "parentId"
              }
            }
          ],
          "array_relationships": [
            {
              "name": "Products",
              "using": {
                "foreign_key_constraint_on": {
                  "column": "parentId",
                  "table": {
                    "name": "Product",
                    "schema": "public"
                  }
                }
              }
            }
          ],
          "select_permissions": [
            {
              "role": "anonymous",
              "permission": {
                "columns": [
                  "color",
                  "description",
                  "id",
                  "image",
                  "name",
                  "parentId",
                  "price",
                  "size"
                ],
                "filter": {}
              }
            }
          ]
        },
        {
          "table": {
            "name": "User",
            "schema": "public"
          }
        }
      ],
      "configuration": {
        "connection_info": {
          "database_url": {
            "from_env": "HASURA_GRAPHQL_DATABASE_URL"
          },
          "isolation_level": "read-committed",
          "pool_settings": {
            "connection_lifetime": 600,
            "idle_timeout": 180,
            "max_connections": 50,
            "retries": 1
          },
          "use_prepared_statements": true
        }
      }
    }
  ]
}